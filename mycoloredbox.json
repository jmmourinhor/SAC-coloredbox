{
	"id": "com.sap.sample.coloredbox",
	"version": "23.0.0",
	"name": "yet another colored box",
	"description": "my colored box",
	"newInstancePrefix": "MyColoredBox",
	"icon": "data:image/png;base64,",
	"vendor": "SAP",
	"eula": "",
	"license": "",
	"webcomponents": [
		{
			"kind": "main",
			"tag": "com-sap-mycoloredbox",
			"url": "https://jmmourinhor.github.io/coloredbox/mycoloredbox.js",
			"integrity": "",
			"ignoreIntegrity": true
		},
		{
			"kind": "styling",
			"tag": "com-sap-mycoloredbox-styling",
			"url": "https://jmmourinhor.github.io/coloredbox/mycoloredbox_styling.js",
			"integrity": "",
			"ignoreIntegrity": true
		},
		{
			"kind": "builder",
			"tag": "com-sap-mycoloredbox-builder",
			"url": "https://jmmourinhor.github.io/coloredbox/mycoloredbox_builder.js",
			"integrity": "",
			"ignoreIntegrity": true
		}
	],
	"properties": {
		"color": {
			"type": "string",
			"description": "Background color",
			"default": "red"
		},
		"opacity": {
			"type": "number",
			"description": "Opacity",
			"default": 1
		},
		"width": {
			"type": "integer",
			"default": 100
		},
		"height": {
			"type": "integer",
			"default": 100
		}
	},
	"methods": {
		"setColor": {
			"description": "Sets the background color.",
			"parameters": [
				{
					"name": "newColor",
					"type": "string",
					"description": "The new background color"
				}
			],
			"body": "this.color = newColor;"
		},
		"getColor": {
			"returnType": "string",
			"description": "Returns the background color.",
			"body": "return this.color;"
		}
	},
	"events": {
		"onClick": {
			"description": "Called when the user clicks the Colored Box."
		}
	}
}
